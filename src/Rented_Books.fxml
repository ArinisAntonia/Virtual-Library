<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RentedBooks">
    <TableView fx:id="tbData" prefHeight="337.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="title" prefWidth="125.0" text="Title" />
            <TableColumn fx:id="author" prefWidth="125.0" text="Author" />
            <TableColumn fx:id="ISBN" prefWidth="125.0" text="ISBN" />
            <TableColumn fx:id="publisher" prefWidth="100.0" text="Publisher" />
         <TableColumn fx:id="return_date" prefWidth="120.0" text="Return Date" />
        </columns>
    </TableView>
   <Button fx:id="back_to_primary" layoutX="30.0" layoutY="353.0" mnemonicParsing="false" onAction="#change_scene" text="Back" />

</AnchorPane>
